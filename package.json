{
  "name": "nextjs-sandbox",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.11.1",
    "npm": "Use pnpm instead of npm for this project.",
    "yarn": "Use pnpm instead of yarn for this project.",
    "pnpm": "8.15.5"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage",
    "check": "run-s check:*",
    "check:format": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "check:format-config": "prettier \"./*.{js,mjs,ts,json}\" \"./configs/**/*.js\" \"./.storybook/*.ts\" --check",
    "check:lint-css": "stylelint \"./src/**/*.{ts,tsx}\"",
    "check:lint-js": "eslint \"./src/**/*.{ts,tsx}\"",
    "check:spell": "cspell \"./src/**/*.{ts,tsx}\"",
    "check:spell-config": "cspell \"./*.{js,mjs,ts,json}\" \"./configs/**/*.js\" \"./.storybook/*.ts\"",
    "fix": "run-s fix:*",
    "fix:format": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:format-config": "prettier \"./*.{js,mjs,ts,json}\" \"./configs/**/*.js\" \"./.storybook/*.ts\" --write",
    "fix:lint-css": "stylelint \"./src/**/*.{ts,tsx}\" --fix",
    "fix:lint-js": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "next": "14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.1.8",
    "the-new-css-reset": "1.11.2",
    "typescript": "5.4.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "8.0.0",
    "@storybook/addon-docs": "8.0.0",
    "@storybook/addon-essentials": "8.0.0",
    "@storybook/addon-interactions": "8.0.0",
    "@storybook/addon-links": "8.0.0",
    "@storybook/addon-themes": "8.0.0",
    "@storybook/blocks": "8.0.0",
    "@storybook/jest": "0.2.3",
    "@storybook/nextjs": "8.0.0",
    "@storybook/testing-library": "0.2.2",
    "@swc/core": "1.4.8",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "cspell": "8.6.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-security": "2.1.1",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-strict-dependencies": "1.3.4",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "51.0.1",
    "jest": "29.7.0",
    "jest-styled-components": "7.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "storybook": "8.0.0",
    "stylelint": "16.2.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "stylelint-order": "6.0.4"
  },
  "browserslist": [
    "default"
  ]
}
